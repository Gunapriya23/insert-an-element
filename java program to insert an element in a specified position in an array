import java.util.*;
class HelloWorld {
    public static void main(String[] args) {
        int[] arr={8,9,6,5,3};
        int[] newarr=new int[arr.length+1];
        int position=3;
        int element=90;
        for(int i=0;i<position;i++){
            newarr[i]=arr[i];
        }
        newarr[position]=element;
        for(int i=position;i<arr.length;i++){
            newarr[i+1]=arr[i];
        }
        System.out.println(Arrays.toString(newarr));
    }
}
